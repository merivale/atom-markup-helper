{
  "name": "atom-markup-helper",
  "main": "./lib/atom-markup-helper",
  "version": "1.0.0",
  "description": "Markup keyboard shortcuts for Atom (for HTML, Markdown, and Markit)",
  "repository": {
    "type": "git",
    "url": "https://github.com/merivale/atom-markup-helper"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "atom-markup-helper:strong",
      "atom-markup-helper:emphasis",
      "atom-markup-helper:bold",
      "atom-markup-helper:italics",
      "atom-markup-helper:underlined",
      "atom-markup-helper:aside",
      "atom-markup-helper:deleted",
      "atom-markup-helper:inserted",
      "atom-markup-helper:superscript",
      "atom-markup-helper:subscript"
    ]
  }
}
